const title = "Page not found â€¢ Vitamin Brand Checklist";
const description = "The page you're looking for doesn't exist.";
const canonical = "https://vitaminbrandindex.com/404";
---
<Base {title} {description} {canonical}>
  <main class="container stack-lg" style="text-align:center;padding:4rem 1rem;">
    <header class="page-header">
      <h1>Page not found</h1>
      <p class="lead">The page you're looking for doesn't exist or has been moved.</p>
